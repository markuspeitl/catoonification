		<div class="wrap">
			<h2>Select <small> the filter you want to use</small></h2>

			<div class="scrollbar">
				<div class="handle">
					<div class="mousearea"></div>
				</div>
			</div>

			<div class="frame" id="forcecentered">
				<ul class="clearfix">
					{{#each filters}}
                        <li><div><img src="img/filters/{{this}}.jpg" onerror="this.style.display='none'" />{{@index}}</div><p>{{this}}</p></li>
                    {{/each}}
				</ul>
			</div>

			<div class="controls center">
				<button class="btn prev"><i class="icon-chevron-left"></i> prev</button>
